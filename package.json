{
  "name": "@bluvo/sdk",
  "private": true,
  "scripts": {
    "complete-sync": "pnpm --filter @bluvo/sdk-ts build && pnpm --filter @bluvo/react build",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "openapi-ts": "openapi-ts",
    "generate-sdk": "npx @hey-api/openapi-ts -i https://api-bluvo.com/api/v0/openapi -o packages/ts/generated",
    "sync-sdk-from-openapi-inc-version": "jq '.version |= (split(\".\")|map(tonumber)|.[2] |= .+1|map(tostring)|join(\".\"))' packages/ts/package.json > packages/ts/package.json.tmp && mv packages/ts/package.json.tmp packages/ts/package.json"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.86.8",
    "prettier": "^3.5.3",
    "turbo": "^2.5.0",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@gomomento/sdk-web": "^1.113.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
