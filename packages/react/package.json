  "dependencies": {
    "@bluvo/sdk-ts": "workspace:*"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^18.0.0",
    "typescript": "^5.0.0",
    "vitest": "^3.1.1"
  },
  "files": [
    "dist/**/*",
    "README.md"
