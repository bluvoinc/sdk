{
  "name": "@bluvo/react",
  "version": "1.1.78",
  "description": "React hooks for Bluvo SDK - Framework-agnostic state machine for crypto withdrawals",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "Bluvo Inc.",
  "license": "MIT",
  "homepage": "https://github.com/bluvoinc/sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluvoinc/sdk.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/bluvoinc/sdk/issues"
  },
  "scripts": {
    "dev": "tsc --watch",
