{
  "name": "@bluvo/sdk-ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "version": "1.1.73",
  "description": "Bluvo SDK for TypeScript",
  "author": "Bluvo Inc.",
  "license": "MIT",
  "homepage": "https://github.com/bluvoinc/sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluvoinc/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/bluvoinc/sdk/issues"
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && tsc --module esnext --outDir dist/esm && mv dist/esm/index.js dist/index.esm.js && rm -rf dist/esm",
    "test": "vitest run",
    "test:browser": "vitest run --config vitest.browser.config.ts",
    "test:browser:ui": "vitest run --config vitest.browser.config.ts --ui",
    "test:browser:chromium": "vitest run --config vitest.browser.config.ts --browser=chromium",
    "test:browser:firefox": "vitest run --config vitest.browser.config.ts --browser=firefox",
    "test:browser:webkit": "vitest run --config vitest.browser.config.ts --browser=webkit",
    "prepublishOnly": "pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "bluvo",
    "sdk",
    "typescript",
    "nodejs",
    "api"
  ],
  "dependencies": {
    "@gomomento/sdk-web": "^1.113.0"
  },
  "devDependencies": {
    "@vitest/browser": "^3.2.4",
    "playwright": "^1.56.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}
